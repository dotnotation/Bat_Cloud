<%= form_with(model: @researcher, url: signup_path)  do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <%= f.label :email %>
    <%= f.text_field :email %><br>
    <%= f.label :password %>
    <%= f.password_field :password %><br>

    <% if logged_in? && admin? %>
        <%= f.label :access, "User Type" %>
            <%= f.select :access, [["Admin","admin"],["Member","member"]],{prompt: @user.access.titleize}, {class: "form-select"}%>
    <%= f.submit "Sign Up" %>
<% end %>