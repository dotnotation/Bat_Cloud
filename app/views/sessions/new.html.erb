<h1> LogIn </h1>

<%= form_with(scope: :researcher, url: login_path)  do |f|%>
    <%= f.label :email %>
    <%= f.text_field :email %><br>
    <%= f.label :password %>
    <%= f.password_field :password %><br>
    <%= f.submit "Sign In" %>
<% end %>
<br>
<%= link_to image_tag("btn_google_signin_light_pressed_web.png"), "/auth/google_oauth2" %>
